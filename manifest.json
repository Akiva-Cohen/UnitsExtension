{
    "manifest_version": 3,
    "name": "Units Replacer",
    "description": "Automatically convert units",
    "version": "1.1.0",
    "icons": {
        "16" : "ruler16.png",
        "48" : "ruler48.png",
        "128" : "ruler128.png"
    },
    "content_scripts" : [
        {
            "matches" : ["*://*/*"],
            "all_frams": "true",
            "js" : ["replacer.js"],
            "run_at" : "document_end"  
        }
    ],
    "background" : {},
    "permissions" : []
}